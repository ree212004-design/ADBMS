IMPLEMENTATION CONSTRAINTS

CREATE TABLE Emp30 ( 
emp_id INT NOT NULL PRIMARY KEY, 
Name VARCHAR(10) NOT NULL, 
Age INT CHECK (Age >= 18), 
Email_id VARCHAR(50) UNIQUE, 
Phone INT NULL 
); 

CREATE TABLE empaddress ( 
emp_id INT, 
city VARCHAR(30), 
FOREIGN KEY (emp_id) REFERENCES Emp30(emp_id) 
); 

